<div class="container">
  <h1>Job Board</h1>

  <table class="table">
    <thead>
      <tr>
        <th>ID</th>
        <th>Job title</th>
        <th>Company name</th>
        <th>Job url</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @jobs.each do |job| %>
        <tr>
          <td><%= job.id %>
          <td><%= job.job_title %></td>
          <td><%= job.company_name %></td>
          <td><%= job.job_url %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <%= link_to 'Back', job_boards_path %>
</div>
